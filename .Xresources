! ~/.Xresources
! by https://github.com/HmJustHanna
! keep your configs simple and clean 
! thank you https://www.askapache.com/linux/rxvt-xresources/

! special
*.foreground:   #929292
*.background:   #000000
*.cursorColor:  #929292

! black
*.color0:   #000000
*.color8:   #808080

! red
*.color1:   #80000
*.color9:   #ff000

! green
*.color2:   #008000
*.color10:  #00ff00

! yellow
*.color3:   #808000
*.color11:  #ffff00

! blue
*.color4:   #000080
*.color12:  #0000ff

! magenta
*.color5:   #800080
*.color13:  #ff00ff

! cyan
*.color6:   #008080
*.color14:  #00ffff

! white
*.color7:   #929292
*.color15:  #929292

! URXVT

URxvt.font:             xft:Hack Nerd Font:size=14
URxvt.boldFont:         xft:Hack Nerd Font:bold:size=14

! URxvt.font:             xft:DejaVu Sans Mono:size=12
! URxvt.boldFont:         xft:DejaVu Sans Mono:bold:size=12

URxvt.visualBell:       false
! URxvt.pointerBlank:     true

URxvt*scrollBar:        false
URxvt*cursorBlink:      false
URxvt*saveLines:        5000
URxvt*internalBorder:   0
URxvt*transparent:      1
URxvt*shading:          10
! URxvt*fading:           30

! KEYBINDINGS

URxvt.iso14755:                 false

! EXTENSIONS

URxvt.perl-ext-common:  default,font-size

