! special
*.foreground:   #929292
*.background:   #000000
*.cursorColor:  #929292

! black
*.color0:   #000000
*.color8:   #808080

! red
*.color1:   #80000
*.color9:   #ff000

! green
*.color2:   #008000
*.color10:  #00ff00

! yellow
*.color3:   #808000
*.color11:  #ffff00

! blue
*.color4:   #000080
*.color12:  #0000ff

! magenta
*.color5:   #800080
*.color13:  #ff00ff

! cyan
*.color6:   #008080
*.color14:  #00ffff

! white
*.color7:   #c0c0c0
*.color15:  #ffffff

! URXVT

URxvt.font:             xft:DejaVu Sans Mono:size=14
URxvt.boldFont:         xft:DejaVu Sans Mono:bold:size=14
URxvt.visualBell:       true

URxvt*scrollBar:        false
URxvt*cursorBlink:      false
URxvt*saveLines:        5000
URxvt*internalBorder:   5

! keybindings

URxvt.iso14755:                 false
!URxvt.keysym.Shift-Control-V:   eval:paste_clipboard
!URxvt.keysym.Shift-Control-C:   eval:selection_to_clipboard

! extensions

! URxvt.perl-lib:         /home/anna/.config/urxvt

URxvt.perl-ext-common:  default,font-size,keyboard-select,selection-to-clipboard

! url-select

! URxvt.urlLauncher:          /usr/bin/firefox
! URxvt.url-select.underline: true
! URxvt.keysym.M-u:           url-select:select_next

! keyboard-select

URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
! URxvt.keyboard-select.clipboard: true
